<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SmokeTestsChrome" parallel="methods">
    <test name="Smoke" preserve-order="true" thread-count="3">
        <groups>
            <run>
                <include name="smoke" />
            </run>
        </groups>
        <packages>
            <package name="test.scenarios.*" />
        </packages>
    </test>
</suite>